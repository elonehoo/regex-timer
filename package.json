{
  "name": "@elonehoo/regex-timer",
  "type": "module",
  "version": "0.0.1",
  "description": "Make a regular expression time out if it takes too long to execute",
  "keywords": [
    "regex",
    "timer"
  ],
  "license": "MIT",
  "author": "Elone Hoo <huchengyea@163.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elonehoo/regex-timer.git"
  },
  "funding": "https://github.com/sponsors/elonehoo",
  "homepage": "https://github.com/elonehoo/regex-timer#readme",
  "bugs": {
    "url": "https://github.com/elonehoo/regex-timer/issues"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "rimraf dist && unbuild",
    "prepublishOnly": "npm run build",
    "release": "bumpp package.json --commit --push --tag && pnpm -r publish --access public",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "clone-regexp": "^3.0.0",
    "function-timeout": "^0.1.1",
    "time-span": "^5.1.0"
  },
  "devDependencies": {
    "@elonehoo/eslint-config": "2.0.0",
    "@elonehoo/rimraf": "0.0.1",
    "@types/node": "18.15.11",
    "@vitest/coverage-c8": "0.30.1",
    "bumpp": "9.1.0",
    "c8": "7.13.0",
    "eslint": "8.38.0",
    "pnpm": "8.2.0",
    "typescript": "5.0.4",
    "unbuild": "^1.2.1",
    "vite": "4.2.1",
    "vitest": "0.30.1"
  }
}
