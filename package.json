{
  "name": "@elonehoo/regex-timer",
  "type": "module",
  "version": "0.0.4",
  "description": "Make a regular expression time out if it takes too long to execute",
  "author": "Elone Hoo <huchengyea@163.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/elonehoo",
  "homepage": "https://github.com/elonehoo/regex-timer#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elonehoo/regex-timer.git"
  },
  "bugs": {
    "url": "https://github.com/elonehoo/regex-timer/issues"
  },
  "keywords": [
    "regex",
    "timer"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && unbuild",
    "prepublishOnly": "npm run build",
    "release": "bumpp package.json --commit --push --tag && pnpm -r publish --access public",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "clone-regexp": "^3.0.0",
    "function-timeout": "^1.0.2",
    "time-span": "^5.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "5.3.0",
    "@elonehoo/rimraf": "0.0.1",
    "@types/node": "22.18.3",
    "@vitest/coverage-v8": "^3.2.4",
    "bumpp": "10.2.3",
    "c8": "10.1.3",
    "eslint": "9.35.0",
    "pnpm": "10.16.1",
    "typescript": "5.9.2",
    "unbuild": "^3.6.1",
    "vite": "7.1.5",
    "vitest": "3.2.4"
  }
}
