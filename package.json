{
  "name": "@elonehoo/regex-timer",
  "version": "0.0.4",
  "description": "Make a regular expression time out if it takes too long to execute",
  "author": "Elone Hoo <huchengyea@163.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/elonehoo",
  "homepage": "https://github.com/elonehoo/regex-timer#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elonehoo/regex-timer.git"
  },
  "bugs": {
    "url": "https://github.com/elonehoo/regex-timer/issues"
  },
  "keywords": [
    "regex",
    "timer"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && unbuild",
    "prepublishOnly": "npm run build",
    "release": "bumpp package.json --commit --push --tag && pnpm -r publish --access public",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "clone-regexp": "^3.0.0",
    "function-timeout": "^1.0.1",
    "time-span": "^5.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.13.3",
    "@elonehoo/rimraf": "0.0.1",
    "@types/node": "20.12.7",
    "@vitest/coverage-v8": "^1.5.0",
    "bumpp": "9.4.0",
    "c8": "9.1.0",
    "eslint": "8.57.0",
    "pnpm": "8.15.7",
    "typescript": "5.4.5",
    "unbuild": "^2.0.0",
    "vite": "5.2.8",
    "vitest": "1.5.0"
  }
}
