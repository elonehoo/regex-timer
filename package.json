{
  "name": "regex-timer",
  "version": "0.0.0",
  "type": "module",
  "description": "Make a regular expression time out if it takes too long to execute",
  "funding": "https://github.com/sponsors/elonehoo",
  "homepage": "https://github.com/elonehoo/regex-timer#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elonehoo/regex-timer.git"
  },
  "bugs": {
    "url": "https://github.com/elonehoo/regex-timer/issues"
  },
  "keywords": [
    "regex",
    "timer"
  ],
  "author": "Elone Hoo <huchengyea@163.com>",
  "license": "MIT",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.js",
  "types": "./dist/type.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && vite build",
    "release": "bumpp package.json --commit --push --tag && pnpm -r publish --access public",
    "test": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@elonehoo/eslint-config": "^2.0.0",
    "@types/node": "^18.7.9",
    "bumpp": "^8.2.1",
    "eslint": "^8.22.0",
    "pnpm": "^7.9.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4",
    "vite": "^3.0.9",
    "vitest": "^0.22.1"
  },
  "dependencies": {
    "clone-regexp": "^3.0.0",
    "function-timeout": "^0.1.1",
    "time-span": "^5.1.0"
  }
}
